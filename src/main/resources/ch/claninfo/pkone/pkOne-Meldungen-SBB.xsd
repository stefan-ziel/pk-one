			<xs:documentation xml:lang="de">
				Berechnungeergebnisse sind entweder Zahlen oder Datumswerte
			</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:decimal xs:dateTime" />
	</xs:simpleType>

	<xs:element name="Simulation">
		<xs:annotation>
			<xs:documentation xml:lang="de">
				Ein Key-Value-Paar aus der Berechung der Simulation
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Name" type="pko:SimParam">
					<xs:annotation>
						<xs:documentation xml:lang="de">
							Name des Wertes
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Wert" type="pko:SimValue">
					<xs:annotation>
						<xs:documentation xml:lang="de">
							Berechneter Wert
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="Meldung">
		<xs:annotation>
			<xs:documentation xml:lang="de">
				Informierende oder warnende Meldungen aus PVCLAN. Berechnug wurde durchgeführt.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Nummer" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="de">
						Meldungsnummer
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Text" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="de">
						Meldungstext in der angegebenen Sprache
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>

	<xs:element name="Fehler">
		<xs:annotation>
			<xs:documentation xml:lang="de">
				Fehlermeldungen aus PVCLAN. Berechnug wurde nicht durchgeführt.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Meldung" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="de">
						Meldungstext in der angegebenen Sprache
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Details" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="de">
						Weiterführende Informationen zu diesem Fehler
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	
	<xs:complexType name="ExecuteResponseType">
		<xs:annotation>
			<xs:documentation xml:lang="de">
				Normale Antwort auf eine "Execute"-Meldung
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="pko:WFMessageType">
				<xs:sequence>
					<xs:element ref="pko:Simulation" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="pko:Meldung" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="pko:Fehler" minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="EINType">
		<xs:annotation>
			<xs:documentation xml:lang="de">
				Antwort bei Einlagensimulation
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="pko:ExecuteResponseType">
				<xs:sequence>
					<xs:element name="Applikation" minOccurs="0">
						<xs:complexType>
							<xs:attribute type="xs:decimal" name="EKOFF.max" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Maximale EKOFFsumme</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute type="xs:decimal" name="EKOFF.min" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Minimale EKOFFsumme</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute type="xs:decimal" name="VAROFF.max" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Maximale VAROFFsumme</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute type="xs:decimal" name="VAROFF.min" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Minimale VAROFFsumme</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute type="xs:decimal" name="UPENOFF.max" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Maximale UPENOFFsumme</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute type="xs:decimal" name="UPENOFF.min" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Minimale UPENOFFsumme</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="KPType">
		<xs:annotation>
			<xs:documentation xml:lang="de">
				Antwort bei Kapitalbezugsimulation
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="pko:ExecuteResponseType">
				<xs:sequence>
					<xs:element name="Applikation" minOccurs="0">
						<xs:complexType>
							<xs:attribute type="xs:decimal" name="KPS.max" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Maximale KPsumme</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute type="xs:decimal" name="KPS.min" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Aktuelle KPsumme</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="WEFType">
		<xs:annotation>
			<xs:documentation xml:lang="de">
				Antwort bei Kapitalbezugsimulation
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="pko:WFMessageType">
				<xs:sequence>
					<xs:element name="Applikation" minOccurs="0">
						<xs:complexType>
							<xs:attribute type="xs:decimal" name="WEF.max" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Maximale
										WEFsumme
									</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute type="xs:decimal" name="WEF.min" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Aktuelle
										WEFsumme
									</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
					<xs:element ref="pko:Simulation" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="pko:Meldung" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="pko:Fehler" minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	
	<xs:complexType name="EKType">
		<xs:complexContent>
			<xs:extension base="pko:WFMessageType">
				<xs:sequence>
					<xs:element name="Applikation" minOccurs="0">
						<xs:complexType>
							<xs:attribute type="xs:decimal" name="EKS.max" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Maximale
										Einkaufssumme
									</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute type="xs:decimal" name="EKS" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="de">Aktuelle
										Einkaufssumme
									</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
					<xs:element ref="pko:Simulation" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="pko:Meldung" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="pko:Fehler" minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="DocumentType">
		<xs:complexContent>
			<xs:extension base="pko:WFMessageType">
				<xs:sequence>
					<xs:element name="Dokument" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
								Das Dokument wird als Elementinhalt in
								BASE 6 to 4 Kodierung übergeben.
							</xs:documentation>
						</xs:annotation>
						<xs:complexType mixed="true">
							<xs:attribute name="Name" type="xs:string" use="optional" />
							<xs:attribute name="MimeType" type="xs:string" use="optional" />
						</xs:complexType>
					</xs:element>
					<xs:element ref="pko:Fehler" minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!-- Requests und Responses -->


	<xs:element name="EKSimStartRequest" type="pko:StartMessageType">
		<xs:annotation>
			<xs:documentation>Start einer Einkaufssimulation</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="EKSimStartResponse" type="pko:EKType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommen die Berechneten Werte vor
				Einkauf zurück die in Portal STEP 3 angezeigt werden sollen
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="EKSimExecuteRequest">
		<xs:annotation>
			<xs:documentation>Übergabe des Einkaufsbetrags</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pko:WFMessageType">
					<xs:attribute type="xs:decimal" name="EKS" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Simulativer
								Einkaufsbetrag
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="EKSimExecuteResponse" type="pko:EKType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommen die Berechneten Werte nach
				Einkauf zurück die in Portal STEP 3 angezeigt werden sollen
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="EKSimEndRequest" type="pko:WFMessageType">
		<xs:annotation>
			<xs:documentation>Beenden der Einkaufssimulation</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="EKSimEndResponse" type="pko:DocumentType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommt ein VA in PDF-Format
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<!-- WEF 2 Stepper -->
	<xs:element name="WEFSimStartRequest" type="pko:StartMessageType">
		<xs:annotation>
			<xs:documentation>Start einer WEFsimulation</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="WEFSimStartResponse" type="pko:WEFType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommen die Berechneten Werte vor
				WEF zurück die in Portal STEP 3 angezeigt werden sollen
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="WEFSimExecuteRequest">
		<xs:annotation>
			<xs:documentation>Übergabe des WEF-Betrags</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pko:WFMessageType">
					<xs:attribute type="xs:decimal" name="WEF" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Simulativer
								WEFbetrag
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute type="xs:dateTime" name="ValutaNeu" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Valutadatum /
								Berechnungsdatum
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute type="xs:decimal" name="EndSimulation" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Indikator, dass
								Simulation beendet werden soll, immer 2 im EndRequest!
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="WEFSimExecuteResponse" type="pko:WEFType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommen die Berechneten Werte nach
				WEF zurück die in Portal STEP 3 angezeigt werden sollen
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="WEFSimEndRequest">
		<xs:annotation>
			<xs:documentation>Beenden der WEF Simulation</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pko:WFMessageType">
					<xs:attribute type="xs:decimal" name="EndSimulation" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Indikator, dass
								Simulation beendet werden soll, immer 2 im EndRequest!
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="WEFSimEndResponse" type="pko:DocumentType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommen die WEFDokumente in PDF-Format
			</xs:documentation>
		</xs:annotation>
	</xs:element>


	<!-- SIM VA 1 Stepper -->
	<xs:element name="VASimStartRequest" type="pko:StartMessageType">
		<xs:annotation>
			<xs:documentation>Start eines VA</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="VASimStartResponse" type="pko:ExecuteResponseType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommen die Berechneten Werte
				zurück
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="VASimExecuteRequest">
		<xs:annotation>
			<xs:documentation>Übergabe des Flags Weiter, mit 1 wird der WF
				abgeschlossen und das Dokument bezogen
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pko:WFMessageType">
					<xs:attribute type="xs:decimal" name="EndSimulation" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Immer 1 im Executerequest
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute type="xs:dateTime" name="ValutaNeu" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Valutadatum /
								Berechnungsdatum
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="VASimExecuteResponse" type="pko:DocumentType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommt ein VA in PDF-Format
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="VASimEndRequest">
		<xs:annotation>
			<xs:documentation>Beenden der VA Simulation</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pko:WFMessageType">
					<xs:attribute type="xs:decimal" name="EndSimulation" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Indikator, dass
								Simulation beendet werden soll, immer 2 im EndRequest!
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="VASimEndResponse" type="pko:DocumentType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommt ein VA in PDF-Format
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<!-- SIM RE 1 Stepper -->
	<xs:element name="RESimStartRequest">
		<xs:annotation>
			<xs:documentation>Eine Rentensimulation</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pko:MessageType">
					<xs:attribute type="xs:string" name="Mitnum" use="required" />
					<xs:attribute type="xs:dateTime" name="Geburtsdatum" use="required" />
					<xs:attribute name="Sparplan" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Standard" />
								<xs:enumeration value="Plus" />
								<xs:enumeration value="Minus" />
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute type="xs:dateTime" name="Stichtag" use="required" />
					<xs:attribute type="xs:double" name="Jahreslohn" use="required" />
					<xs:attribute type="xs:double" name="Sparkapital" use="required" />
					<xs:attribute type="xs:dateTime" name="PensDatVor" use="required" />
					<xs:attribute type="xs:dateTime" name="PensDatNach" use="required" />
					<xs:attribute type="xs:double" name="Projektionszinssatz" use="required" />
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="RESimStartResponse" type="pko:ExecuteResponseType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommen die Berechneten Werte
				zurück
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<!-- 2 Stepper Einlage -->
	<xs:element name="EINSimStartRequest" type="pko:StartMessageType">
		<xs:annotation>
			<xs:documentation>Start einer Einlagensimulation</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="EINSimStartResponse" type="pko:EINType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommen die Berechneten Werte vor
				Einlage zurück die in Portal STEP 3 angezeigt werden sollen
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="EINSimExecuteRequest">
		<xs:annotation>
			<xs:documentation>Übergabe der Einlagenwerte</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pko:WFMessageType">
					<xs:attribute type="xs:decimal" name="EndSimulation" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Immer 1 im Executerequest
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute type="xs:decimal" name="EKOFF" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Altersguthaben
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute type="xs:decimal" name="RATEOFF" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="de">Altersguthaben monatlich
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute type="xs:decimal" name="VAROFF" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Konto vorzeitiger Altersrücktritt
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute type="xs:decimal" name="UPENOFF" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Konto Überbrückungspension
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute type="xs:dateTime" name="ValutaNeu" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Valutadatum /
								Berechnungsdatum
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="EINSimExecuteResponse" type="pko:EINType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommen die Berechneten Werte nach
				Einlagen zurück die in Portal STEP 3 angezeigt werden sollen
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="EINSimEndRequest">
		<xs:annotation>
			<xs:documentation>Beenden der Einlagen Simulation</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pko:WFMessageType">
					<xs:attribute type="xs:decimal" name="EndSimulation" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Indikator, dass
								Simulation beendet werden soll, immer 2 im EndRequest!
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="EINSimEndResponse" type="pko:DocumentType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommen die Einlagendokumente in PDF-Format
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<!-- freiwilliger Sparbeitrag 2 Stepper -->
	<xs:element name="FRSSimStartRequest" type="pko:StartMessageType">
		<xs:annotation>
			<xs:documentation>Start einer Einlagensimulation</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="FRSSimStartResponse" type="pko:ExecuteResponseType">
		<xs:annotation>
			<xs:documentation>In dieser Antwort kommen die Berechneten Werte für den freiwilligen Sparbeitrag
			</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="FRSSimExecuteRequest">
		<xs:annotation>
			<xs:documentation>Übergabe der Werte für den freiwilligen Sparbeitrag</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="pko:WFMessageType">
					<xs:attribute type="xs:decimal" name="EndSimulation" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Immer 1 im Executerequest
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute type="xs:decimal" name="PJN" use="required">
						<xs:annotation>
							<xs:documentation xml:lang="de">Ja Nein
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute type="xs:dateTime" name="ValutaNeu" use="required">
						<xs:annotation>
